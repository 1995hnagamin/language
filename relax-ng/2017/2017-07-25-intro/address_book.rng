<element name="addressBook" xmlns="http://relaxng.org/ns/structure/0.9">
  <zeroOrMore>
    <element name="card">
      <choice>
        <attribute name="name">
          <text />
        </attribute>
          <group>
            <attribute name="givenName">
              <text />
            </attribute>
            <attribute name="familyName">
              <text />
            </attribute>
          </group>
      </choice>
      <attribute name="email">
        <text />
      </attribute>
      <optional>
        <attribute name="note">
          <text />
        </attribute>
      </optional>
    </element>
  </zeroOrMore>
</element>
